{
	"compilerOptions": {
		"target": "es2021",
		//"module": "nodenext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,

		"jsx": "react",
		"strict": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"paths": {
			"*": ["./src/types/*"]
		}
	}
}
